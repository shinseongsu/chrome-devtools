    "mime": "^4.0.1",
    "reconnecting-websocket": "^4.4.0",
    "string-random": "^0.1.3",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "ws": "^7.5.9"
  }
